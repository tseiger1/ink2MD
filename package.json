{
  "name": "ink2md",
  "version": "0.1.1",
  "description": "Import handwritten notes from disk, convert pages to PNG, and ask a vision LLM to produce Markdown.",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint .",
    "test": "jest"
  },
  "keywords": [
    "obsidian",
    "ink2md",
    "vision"
  ],
  "license": "0-BSD",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.30.1",
    "@jest/globals": "^30.2.0",
    "@types/color": "^4.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.10",
    "esbuild": "0.25.5",
    "eslint-plugin-obsidianmd": "0.1.9",
    "globals": "14.0.0",
    "jest": "^29.7.0",
    "jiti": "2.6.1",
    "ts-jest": "^29.2.5",
    "tslib": "2.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.35.1"
  },
  "dependencies": {
    "@google/genai": "^1.38.0",
    "jszip": "^3.10.1",
    "obsidian": "latest",
    "openai": "^6.16.0",
    "pdfjs-dist": "^5.4.530"
  }
}
